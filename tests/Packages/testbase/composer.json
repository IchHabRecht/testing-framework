{
    "name": "ichhaberecht/testing-framework-testbase",
    "type": "typo3-cms-extension",
    "description": "Testbase extension for ichhaberecht/testing-framework",
    "homepage": "https://github.com/IchHabRecht/testing-framework",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": ">=6.2 <9.0",
        "typo3/cms-fluid": ">=6.2 <9.0"
    },
    "autoload": {
        "psr-4": {
            "IchHabRecht\\Testbase\\": "Classes/",
            "IchHabRecht\\Testbase\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "testbase": "self.version"
    }
}
